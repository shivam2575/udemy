<%- include("header"); -%>
  <body>
    <div class="box" id="heading">
        <h1> <%= listTitle %> </h1>
    </div>


  <div class="box">

    <div class="box">

      <%  for(var i=0;i<newListItem.length;i++){ %>
        <div class="item">

          <input type="checkbox">
            <p> <%= newListItem[i] %> </p>

        </div>

      <%  } %>
    </div>

    <form class="" action="/" method="post">
      <input type="text" name="newItem" placeholder="New Item">
      <button type="submit" name="list" value=<%= listTitle %>>+</button>
    </form>


  </div>
  </body>

  <%- include("footer"); -%>
